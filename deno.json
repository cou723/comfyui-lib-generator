{
  "name": "@coucha/comfyui-client-generator",
  "version": "0.2.0",
  "description": "Generate type-safe TypeScript bindings for ComfyUI nodes with compile-time type checking and IDE autocomplete",
  "keywords": [
    "comfyui",
    "code-generator",
    "typescript",
    "dsl",
    "type-safe",
    "workflow",
    "node-bindings",
    "stable-diffusion",
    "ai-image-generation"
  ],
  "exports": {
    ".": "./src/cli.ts"
  },
  "tasks": {
    "generate": "deno run -A src/cli.ts",
    "type-check": "deno check src/cli.ts src/generator.ts src/scanner.ts",
    "test": "deno run -A test/test.ts"
  },
  "license": "MIT",
  "publish": {
    "exclude": [
      "examples/**"
    ]
  }
}
